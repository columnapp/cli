{
  "name": "@columnapp/cli",
  "version": "1.0.0-alpha2",
  "repository": "https://github.com/columnapp/cli",
  "scripts": {
    "test-init": "ts-node init.ts",
    "dev-server": "npx http-server -c-1",
    "test-publish": "ts-node publish.ts --path publish-test/",
    "clean": "rimraf ./dist/",
    "cli": "ts-node ./index.ts",
    "build": "yarn run clean && tsc",
    "prepare": "yarn run build"
  },
  "bin": {
    "cli": "./dist/index.js"
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "author": {
    "email": "hello@column.app",
    "name": "columnapp",
    "url": "https://column.app"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "axios": "^1.2.0",
    "colors": "^1.4.0",
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "enquirer": "^2.3.6",
    "esbuild": "^0.15.15"
  },
  "devDependencies": {
    "@columnapp/schema": "0.0.1-alpha14",
    "@types/node": "^18.11.9",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
